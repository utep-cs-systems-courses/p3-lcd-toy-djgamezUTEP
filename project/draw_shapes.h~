#ifndef _DRAW_SHAPES_H_
#define _DRAW_SHAPES_H_

/* draws a rectangle */
void draw_rectangle(void);
void draw_rectangle2(void);
void draw_rectangle3(void);
void draw_rectangle4(void);
void draw_rectangle5(void);
void draw_rectangle6(void);
void draw_rectangle7(void);
/* draws an equaleral triangle by starting at the top with a 
   width of 1 and increasing the width by 1
   for every row */
void draw_triangle(void);
/* draws a circle using bresenham's algorithm */
void draw_circle(void);
void draw_circle2(void);
void draw_circle3(void);
void draw_circle4(void);
void draw_circle5(void);
void draw_BTC(int xShift, int yShift);
#endif // _DRAW_SHAPES_H_
